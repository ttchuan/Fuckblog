<%- include header %>
<% posts.forEach(function (post,index){ %>
<p><h2><a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h2></p>
<p>
	<%- post.post %>
</p>
<p class="info">
	<p>作者：<a href="/u/<%= post.name %>"><%= post.name %></a></p>
	<p>日期：<%= post.time.minute %></p>
</p>
<% }) %>
<%- include footer %>
